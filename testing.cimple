// Enable Input and Output system
start IO;

// Start main function
startMain();

// Code here:
int num1;
int num2;
int result;

print("Enter a number: \n");
input("%i", &num1);

print("Enter other name: \n");
input("%i", &num2);

result = num1 + num2;

print("The result is: %i", result);

// End main function
endMain();